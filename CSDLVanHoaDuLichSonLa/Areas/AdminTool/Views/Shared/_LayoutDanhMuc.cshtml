<!DOCTYPE html>
<html class="loading" lang="vi" data-textdirection="ltr">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="Hệ thống quản lý cơ sở dữ liệu Văn hóa, Thể thao và Du lịch tỉnh Sơn La">
    <meta name="keywords" content="Hệ thống quản lý cơ sở dữ liệu Văn hóa, Thể thao và Du lịch tỉnh Sơn La">
    <title>@ViewBag.Title - Hệ thống quản lý cơ sở dữ liệu Văn hóa, Thể thao và Du lịch tỉnh Sơn La</title>
    <link rel="apple-touch-icon" sizes="57x57" href="~/assets/images/favicon/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="~/assets/images/favicon/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/assets/images/favicon/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="~/assets/images/favicon/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/assets/images/favicon/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="~/assets/images/favicon/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="~/assets/images/favicon/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/assets/images/favicon/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="~/assets/images/favicon/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192" href="~/assets/images/favicon/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/assets/images/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/assets/images/favicon/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/assets/images/favicon/favicon-16x16.png">
    @* <link rel="manifest" href="/manifest.json"> *@
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">

    <!-- BEGIN VENDOR CSS-->
    <link rel="stylesheet" type="text/css" href="~/app-assets/css/vendors.css">
    <!-- <link rel="stylesheet" type="text/css" href="~/app-assets/vendors/css/tables/datatable/datatables.min.css" />
    <link rel="stylesheet" type="text/css" href="~/app-assets/vendors/css/selects/select2.min.css" />
    <link rel="stylesheet" type="text/css" href="~/app-assets/vendors/css/selects/select2-bootstrap4.min.css" /> -->

    @RenderSection("VendorCss", required: false)
    <!-- END VENDOR CSS-->
    <!-- BEGIN STACK CSS-->
    <link rel="stylesheet" type="text/css" href="~/app-assets/css/app.css" />
    <!-- END STACK CSS-->
    <!-- BEGIN Page Level CSS-->
    <link rel="stylesheet" type="text/css" href="~/app-assets/css/core/menu/menu-types/vertical-menu.css" />
    <link rel="stylesheet" type="text/css" href="~/assets/vendors/animate/animate.min.css" />
    <link rel="stylesheet" type="text/css" href="~/assets/vendors/FluentSystemIcons/css/FluentSystemIcons.css" />
    <!-- END Page Level CSS-->

    <!--Bootstrap 4.2-->
    <link rel="stylesheet" href="~/tools/bootstrap/css/bootstrap.min.css" />

    <!--datatable-->
    <link rel="stylesheet" href="https://cdn.datatables.net/fixedcolumns/4.3.0/css/fixedColumns.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="~/tools/DataTables/Buttons-2.0.1/buttons.dataTables.min.css">

    <!--select2-->
    <link href="~/tools/select2/select2.min.css" rel="stylesheet">
    <link href="~/tools/select2/select2-bootstrap4.min.css" rel="stylesheet">

    <!--Highchart-->
    <link href="~/tools/Highcharts-11.1.0/code/css/highcharts.css" rel="stylesheet" />

    <!--Datepicker-->
    <link href="~/tools/jqueryui_datepicker/datepicker.css" rel="stylesheet" />

    <!--Animate-->
    <link href="~/tools/animate/animate.min.css" rel="stylesheet" /> 

    <!-- BEGIN Custom CSS-->
    <link rel="stylesheet" type="text/css" href="~/assets/css/_base.css" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="~/assets/css/stylebase.css" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="~/assets/css/adminstyles.css" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="~/assets/css/latofonts.css" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="~/assets/css/hgi_icon.css" asp-append-version="true" />
    <!-- END Custom CSS-->

    @RenderSection("Css", required: false)
</head>
<body class="vertical-layout vertical-menu 2-columns fixed-navbar menu-collapsed layout-custom page-loading" data-open="click" data-menu="vertical-menu" data-col="2-columns">

    <div class="page-loaders">
        <div id="loading-center">
            <div id="loading-center-absolute">
                <div class="object" id="object_one"></div>
                <div class="object" id="object_two"></div>
                <div class="object" id="object_three"></div>
            </div>
        </div>
    </div>

    <!-- fixed-top-->
    @await Html.PartialAsync("_NavBar")
    @await Html.PartialAsync("_MenuBar")
    <div class="app-content content">
        <div class="content-wrapper">
            <div class="box_content">
                <div class="TieuDe">
                    <h2>@ViewBag.Title</h2>
                </div>
                <div class="content-body">
                    <!-- Group Filter-->
                    @await Html.PartialAsync("~/Areas/AdminTool/Views/Shared/_PartialView/SingleFilter.cshtml")
                    @RenderBody()
                </div>

                <!-- Modal add/edit-->
                @await Html.PartialAsync("~/Areas/AdminTool/Views/Shared/_PartialView/ModalAddEditDanhMucChung.cshtml")

                <!-- Modal delete-->
                @await Html.PartialAsync("~/Areas/AdminTool/Views/Shared/_PartialView/ModalDelete.cshtml")
            </div>
        </div>
    </div>

    <!-- BEGIN VENDOR JS-->
    <script src="~/app-assets/vendors/js/vendors.min.js" type="text/javascript"></script>
    <!-- BEGIN VENDOR JS-->
    <!-- BEGIN PAGE VENDOR JS-->
    @RenderSection("VendorScripts", required: false)
    <!-- END PAGE VENDOR JS-->
    <!-- BEGIN STACK JS-->
    <script src="~/app-assets/js/core/app-menu.js" type="text/javascript"></script>
    <script src="~/app-assets/js/core/app.js" type="text/javascript"></script>
    <script src="~/app-assets/js/scripts/customizer.js" type="text/javascript"></script>
    <!-- <script src="~/assets/vendors/notify/bootstrap-notify.min.js" type="text/javascript"></script> -->
    <!-- END STACK JS-->
    <script src="~/PageScripts/Layout.js" type="text/javascript"></script>

    <!--jQuery UI-->
    <script src="~/tools/jqueryui_datepicker/jquery-ui.min.js"></script>
    <script src="~/tools/jqueryui_datepicker/datepicker-vi.js"></script>
    <script src="~/tools/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!--ckeditor-->
    <script src="~/tools/ckeditor4/ckeditor.js"></script>

    <!--datatable-->
    <script src="~/tools/DataTables/jquery.dataTables.min.js"></script>
    <script src="~/tools/DataTables/checkboxes.min.js"></script>
    <script src="~/tools/DataTables/dataTables.rowGroup.min.js"></script>
    <script src="https://cdn.datatables.net/fixedcolumns/4.3.0/js/dataTables.fixedColumns.min.js"></script>
    <script src="~/tools/DataTables/Buttons-2.0.1/dataTables.buttons.min.js"></script>
    <script src="~/tools/DataTables/JSZip-2.5.0/jszip.min.js"></script>
    <script src="~/tools/DataTables/Buttons-2.0.1/buttons.html5.min.js"></script>

    <!--select2-->
    <script src="~/tools/select2/select2.min.js"></script>

    <!--Highchart-->
    <script src="~/tools/Highcharts-11.1.0/code/highcharts.js"></script>
    
    <!--Notify-->
    <script src="~/js/bootstrap-notify.min.js"></script>

    <!--Validate-->
    <script src="~/js/validate.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>